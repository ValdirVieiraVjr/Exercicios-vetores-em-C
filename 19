/*Leia um vetor de 10 posic ̧oes e atribua valor 0 para todos os elementos que possu  ̃  ́ırem
valores negativos.*/


#include <stdio.h>

int main() {

  int v[10], i;
  
  printf("insira 10 numeros:\n");

  for(i=0; i<10; i++){

    scanf("%d", &v[i]);

    if(v[i] < 0) v[i] = 0;
  }

  for(i=0; i<10; i++){

    printf("%d ", v[i]);
  }
}
